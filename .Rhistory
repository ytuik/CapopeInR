a = (match(Gin2018,Gin2019)) #Comparação para ver se algum grupo se repete nos dois anos
resultado = c()
for(x in a){
if (x =! NA){
resultado = c(resultado, as.character(Gin2019[x]))
}
}
print(unique(resultado))
